package Unit_02;

/*
 * Abstract class must be declared in Abstract keyword
 * 
 * -Abstraction is the process of hiding the implementing  details
 * 
 * 
 * 
 */
abstract class Bike {
	
	int a;
	
	Bike() {
		System.out.println("Inside Bike Constructor: " + a + "/n");
	}
	abstract void run();
	
	void display() {
		System.out.println("this is display method");
	}
}
class Honda4 extends Bike{
	void run(){
}
public class P09_Task01_AbstractAndFinalClassInJava {

	public static void main(String[] args) {
		Bike obj = new Honda4();
		obj.display();
	}
}
final class Super{
	
	public int data = 30;
	void display() {
		System.out.println("data");
	}
}
}

