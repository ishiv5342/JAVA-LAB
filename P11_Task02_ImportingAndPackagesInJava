package Unit_02;//inside this package we can create many classes;

/*
 * java package is a group of similar type of classes , interface and sub-packages
 * - package in java can be classified in two forms:
 *    - built in package
 *    - user defined package
 *    
 * - package avoids to help class name collision
 * - package helps us to maintain access protection in java
 * - package are named in reverse order of domain name
 * unit01.javaproject.com ->" com.javaproject.unit01"
 * 
 * import java.util.vector; // import vector class from util package
 * import java.util.*; //import all classes from util package
 * 
 * Static import:
 * 
 */
import static java.lang.System.*;
import static java.lang.Math.*;

import Unit_01.SampleClass1;

public class P11_Task02_ImportingAndPackagesInJava {

	public static void main(String args[]) {
		
		
		out.println("Welcome to Pacakage ");
		
		out.println(sqrt(4));
		System.out.println(pow(2, 2));
		System.out.println(abs(6.3));
		
		SampleClass1 obj = new SampleClass1();
		System.out.println(obj.a);

	}
}
